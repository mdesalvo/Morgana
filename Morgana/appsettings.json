{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LLM": {
    "Provider": "Anthropic",
    "AzureOpenAI": {
      "Endpoint": "_SECURE_OVERRIDE_",
      "ApiKey": "_SECURE_OVERRIDE_",
      "DeploymentName": "_SECURE_OVERRIDE_"
    },
    "Anthropic": {
      "ApiKey": "_SECURE_OVERRIDE_",
      "Model": "_SECURE_OVERRIDE_"
    },
    "MCPServers": [
      {
        "Name": "HardwareCatalog",
        "Type": "InProcess",
        "Enabled": true
      },
      {
        "Name": "SecurityCatalog",
        "Type": "InProcess",
        "Enabled": true
      }
    ],
    "ParameterNormalization": {
      "MinSubstringLength": 4,
      "SimilarityRatioThreshold": 0.3
    }
  },
  "Cauldron": {
    "BaseUrl": "https://localhost:5002"
  }
}