{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LLM": {
    "Provider": "AzureOpenAI",
    "AzureOpenAI": {
      "Endpoint": "_SECURE_OVERRIDE_",
      "ApiKey": "_SECURE_OVERRIDE_",
      "DeploymentName": "_SECURE_OVERRIDE_"
    },
    "Anthropic": {
      "ApiKey": "_SECURE_OVERRIDE_",
      "Model": "_SECURE_OVERRIDE_"
    }
  },
  "Cauldron": {
    "BaseUrl": "https://localhost:5002"
  },
  "Morgana": {
    "MCPServers": [
      {
        "Name": "HardwareCatalog",
        "Type": "SQLite",
        "ConnectionString": "Data/Catalogs/hardware_catalog.db",
        "Enabled": true
      },
      {
        "Name": "SecurityCatalog",
        "Type": "SQLite",
        "ConnectionString": "Data/Catalogs/security_catalog.db",
        "Enabled": true
      }
    ]
  }
}
